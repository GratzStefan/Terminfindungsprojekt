<Window x:Class="Terminfindungsapp.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Terminfindungsapp"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Register" Margin="355,41,355,354" FontSize="20" FontWeight="Bold"/>
        <Label Content="Username:" Margin="272,106,458,299" FontWeight="Bold"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Margin="280,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Password:" Margin="272,158,458,247" FontWeight="Bold"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Margin="280,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Name="btnRegister" Content="Register" Margin="307,241,367,153" Click="btnRegister_Click"/>
        <Button Name="btnLogin" Content="Login-Page" HorizontalAlignment="Left" Margin="162,335,0,0" VerticalAlignment="Top" Height="38" Width="78" Click="btnLogin_Click"/>

    </Grid>
</Window>
