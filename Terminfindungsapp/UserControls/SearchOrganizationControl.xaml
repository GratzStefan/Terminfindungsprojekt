<UserControl x:Class="Terminfindungsapp.UserControls.SearchOrganizationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Terminfindungsapp.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <StackPanel>
            <DockPanel Height="50px">
                <Label DockPanel.Dock="Left" Width="175px" Margin="50, 10, 0, 10" FontWeight="Bold">Search Organization:</Label>
                <TextBox DockPanel.Dock="Right" Name="txtOrganizationName" Margin="0, 10, 50, 10" TextChanged="txtOrganizationName_TextChanged"></TextBox>
            </DockPanel>
            <StackPanel Name="staOrganization">
                
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
